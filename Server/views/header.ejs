				 <div class="guides-menu-blocks">
				 	<%
				 	var i;
				 	var count = 0;
				 	for(i = 0; i < guides.length; i++){
	    				if(count === 4 ) {break;}
	    				count++;
		                var j;
		                for(j = 0; j < guidesImages.length; j++){
